func mean : i32(x : i32[])
	res : i32 = 0
	i : i32 = 0
	len : i32 = call arrlen x

	while (i < len)
		res = res + x[i]
		i = i + 1
	endwhile

	ret res / len
endfunc

proc hello_world()
	call show "Hello, world!!"
endproc

proc main()
	x : i32 = 10
	l : i32[4] = {1,2,3,4}

	call hello_world
	call showln str(x)
	x = call mean l

	?call show "x = "
	call showln str(x)
endproc
